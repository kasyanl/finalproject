<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>FirstServlets</servlet-name>
        <servlet-class>kasyanl.kasyanlfinalproject.servlet.FirstServlets</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstServlets</servlet-name>
        <url-pattern>/time</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>kasyanl.kasyanlfinalproject.servlet.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AllStudents</servlet-name>
        <servlet-class>kasyanl.kasyanlfinalproject.servlet.AllStudents</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AllStudents</servlet-name>
        <url-pattern>/students</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddStudent</servlet-name>
        <servlet-class>kasyanl.kasyanlfinalproject.servlet.AddStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddStudent</servlet-name>
        <url-pattern>/students/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StudentCard</servlet-name>
        <servlet-class>kasyanl.kasyanlfinalproject.servlet.StudentCard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentCard</servlet-name>
        <url-pattern>/student/</url-pattern>
    </servlet-mapping>

</web-app>